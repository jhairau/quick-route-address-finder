{
  "name": "quick-route-address-finder",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepack": "npm run build",
    "prepare": "husky"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "keywords": [],
  "author": "Johnathan Hair",
  "license": "MIT",
  "description": "Quick Route Address Finder",
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/jest": "30.0.0",
    "eslint": "9.32.0",
    "globals": "16.3.0",
    "husky": "^9.1.7",
    "jest": "30.0.5",
    "jiti": "2.5.1",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "ts-jest": "29.4.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.0",
    "vite": "7.1.0",
    "vite-plugin-dts": "4.5.4"
  },
  "dependencies": {
    "zod": "4.0.15"
  },
  "lint-staged": {
    "*.{ts,js,mts}": [
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
